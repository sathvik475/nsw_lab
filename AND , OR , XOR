#include <stdio.h>
#include <string.h>
int main() {
  char *str = "Hello World":
  char str1[12], str2[12], str3[12];
  int i, len;
  len = strlen(str);
  strcpy(str2, str);
  for(i=0; i < len; i++)
  {
    str1[i] = str[i] & 127;
    printf("%c", str1[i]);
  }
  str1[len] '\0';
  printf("\n");
  printf("%s\n", str2);
  for(i=0; i < len; i++)
  {
    str3[i] = str2[i]^ 127;
    str3[i] = str3[i]^ 127;
    printf("%c", str3[i]);
  }
  str3[len] = '\0';
  printf("\n");
  return 0;
}
